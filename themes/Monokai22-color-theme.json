{
	"name": "Monokai22",
	"type": "dark",
	"colors": {
		"editor.background": "#1E1E1E",
		"editorCursor.foreground": "#F8F8F2",
		"editor.foreground": "#F8F8F2",
		"editor.lineHighlightBackground": "#3E3D32",
		"editor.selectionBackground": "#49483E",
		// "editor.findMatchBackground": "#49483E",
		// "editor.findMatchHighlightBorder": "#75715E",
		// "editor.findMatchHighlightBackground": "#49483E",

		"tab.border": "#F8F8F2",
		"editorLineNumber.activeForeground" : "#F8F8F2",
		"terminal.border" : "#F8F8F2",
		"editorWhitespace.foreground": "#75715E",
		"editorIndentGuide.background": "#75715E",
		"editorIndentGuide.activeBackground": "#75715E",
		"editorRuler.foreground":"#75715E",
		"terminal.selectionBackground": "#456BA5",
		"terminalCursor.background": "#7F7F7F",

		"terminal.foreground": "#F8F8F2",
		"terminal.background": "#1E1E1E",
		"terminal.ansiBlack": "#000000",
		"terminal.ansiRed": "#F92672",
		"terminal.ansiGreen": "#A6E22E",
		"terminal.ansiYellow": "#FFE792",
		"terminal.ansiBlue": "#FD971F",
		"terminal.ansiMagenta": "#AE81FF",
		"terminal.ansiCyan": "#66D9EF",
		"terminal.ansiWhite": "#F8F8F2",
		"terminal.ansiBrightBlack": "#75715E",
		"terminal.ansiBrightRed": "#F92672",
		"terminal.ansiBrightGreen": "#A6E22E",
		"terminal.ansiBrightYellow": "#FFE792",
		"terminal.ansiBrightBlue": "#FD971F",
		"terminal.ansiBrightMagenta": "#AE81FF",
		"terminal.ansiBrightCyan": "#66D9EF",
		"terminal.ansiBrightWhite": "#F8F8F2",
		// "terminalCursor.foreground": "",
	},
	"tokenColors": [
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded"
			],
			"settings": {
				"foreground": "#F8F8F2"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#75715E"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#FFE792"
			}
		},
		{
			"scope": [
				"punctuation.definition.template-expression",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#F92672"
			}
		},
		{
			"scope": [
				"meta.template.expression"
			],
			"settings": {
				"foreground": "#F8F8F2"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"scope": "constant.character, constant.other",
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"scope": [
				"variable",
				"variable.language.this.php",
				// "keyword.operator.class.php",
				"keyword.operator.key.php"
			],
			"settings": {
				"foreground": "#F8F8F2",
				"fontStyle": ""
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#F92672"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#F92672",
				"fontStyle": ""
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#66D9EF",
			}
		},
		{
			"scope": [
				"type", "entity.name.type",
			],
			"settings": {
				"foreground": "#A6E22E",
			}
		},
		{
			"scope": [
				"class", "entity.name.class", "entity.name.type.class",
